{
    "builds": [
        {
            "src": "python-client/api/*.py",
            "use": "@vercel/python"
        },
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python-client/api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "python-client/api/"
    }
}