{
    "builds": [
        {
            "src": "python-client/api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "python-client/api/login_data.py",
            "use": "@vercel/python"
        },
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python-client/api/index.py"
        }
    ]
}